#!/usr/bin/env python3

import sys
from class_entry import default_kernel_instance


if __name__ == '__main__':

    job_params, call_params = default_kernel_instance.find_Entry('cli_parser').call('cli_parse', {'arglist': sys.argv})

    ret_value = default_kernel_instance.find_Entry('job').call('execute_job', { 'job_params': job_params, 'call_params': call_params })

    exit(ret_value)
